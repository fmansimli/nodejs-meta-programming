    "morgan": "^1.10.0"
  },
  "keywords": [
    "ts",
    "js"
  ],
  "author": "Farid Mansimli",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
