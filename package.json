{
  "name": "nodejs-meta-origramming",
  "version": "1.0.0",
  "description": "meta programming with typescript",
  "main": "index.js",
  "scripts": {
    "git": "git add . && git commit -m \"$npm_package_name ($npm_package_version) ## $(date) ## \" && git push origin master",
